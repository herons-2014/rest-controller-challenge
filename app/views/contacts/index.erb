<div class="container">
  <h1>Contacts</h1>
  <% @contacts = Contact.all %>
  <% @contacts.each do |contact| %>
    <ul>
      <li><a href="/contacts/<%=contact.id %>/show"><%= contact.name %></a></li>
      <li><%= contact.email %></li>
    </ul>
    <hr>
  <% end %>
  <a href="/contacts/new">Create Contact</a>
</div>
